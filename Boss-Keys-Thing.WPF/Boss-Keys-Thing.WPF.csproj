	  <PackageReference Include="System.IO.Compression" Version="4.3.0" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Boss-Keys-Thing\Boss-Keys-Thing.csproj" />
	</ItemGroup>

